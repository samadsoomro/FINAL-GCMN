{
    "version": 2,
    "functions": {
        "server/index.ts": {
            "includeFiles": "dist/public/**",
            "runtime": "nodejs18.x"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/server/index.ts"
        }
    ]
}