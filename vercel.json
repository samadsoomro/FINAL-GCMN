{
    "version": 2,
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "devCommand": "npm run dev",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/server/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/server/index.ts"
        }
    ]
}